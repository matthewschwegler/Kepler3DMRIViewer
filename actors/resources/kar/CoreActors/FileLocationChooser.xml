<property name="prop:InputFileType" class="ptolemy.kernel.util.ConfigurableAttribute"><configure>The type of the input files.</configure></property>
<property name="prop:OutputFileType" class="ptolemy.kernel.util.ConfigurableAttribute"><configure>The type of the output files.</configure></property>

